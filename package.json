{
  "name": "times-table-js",
  "version": "0.3.0",
  "description": "Interactive rendering of times tables",
  "main": "main.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "publish": "npm run clean && npm run build-prod && npm run gh-pages",
    "build": "webpack --config webpack.dev.js",
    "build-prod": "webpack --config webpack.prod.js",
    "build-prod-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "clean": "rimraf build/**/*",
    "tsc": "tsc --traceResolution",
    "gh-pages": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MathiasLengler/TimesTableJS.git"
  },
  "author": "Mathias Lengler",
  "license": "MIT",
  "dependencies": {
    "assert-never": "^1.2.1",
    "dat.gui": "^0.7.7",
    "prettier": "^2.5.1",
    "stats.js": "^0.17.0",
    "three": "^0.136.0",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.7",
    "@types/stats": "^0.16.30",
    "css-loader": "6.5.1",
    "gh-pages": "3.2.3",
    "html-webpack-plugin": "5.5.0",
    "rimraf": "^3.0.2",
    "style-loader": "3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "4.5.4",
    "webpack": "5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.1",
    "webpack-merge": "^5.8.0"
  }
}
